@model CarroComprasViewModel

@if(Model.CarroCompras.CarroComprasItems.Count > 0)
{
    <li>
        <a>
            <span class="glyphicon glyphicon-shopping-cart"></span>
            <span id="cart-status">
                @Model.CarroCompras.CarroComprasItems.Count
            </span>
        </a>
    </li>
}